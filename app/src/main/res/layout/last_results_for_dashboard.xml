<?xml version="1.0" encoding="utf-8"?>
<androidx.cardview.widget.CardView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    style="@style/dashboard.item">
    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:padding="4dp">
        <TextView
            android:id="@+id/last_results_header"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:layout_constraintTop_toTopOf="parent"
            style="@style/TitleRoboto20Pt"
            android:text="@string/last_results"/>
        <LinearLayout
            app:layout_constraintTop_toBottomOf="@id/last_results_header"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">
           <LinearLayout
               android:id="@+id/last_results_1_container"
               android:layout_width="match_parent"
               android:layout_height="wrap_content"
               android:orientation="vertical">
               <TextView
                   android:id="@+id/last_results_1_header"
                   android:layout_width="match_parent"
                   android:layout_height="wrap_content"
                   style="@style/CaptionRoboto12Pt"
                   tools:text="I Og\u00f3lnopolskie mistrzostwa O\u0142awy w p\u0142ywaniu (2007-12-12)"/>
               <androidx.constraintlayout.widget.ConstraintLayout
                   android:layout_width="match_parent"
                   android:layout_height="wrap_content"
                   android:paddingVertical="5dp">
                   <TextView
                       android:id="@+id/last_results_1_distance"
                       android:layout_width="wrap_content"
                       android:layout_height="wrap_content"
                       app:layout_constraintTop_toTopOf="parent"
                       app:layout_constraintStart_toStartOf="parent"
                       app:layout_constraintBottom_toBottomOf="parent"
                       style="@style/Body1Roboto14Pt"
                       tools:text="50m"/>
                   <TextView
                       android:id="@+id/last_results_1_style"
                       android:layout_width="0dp"
                       android:layout_height="wrap_content"
                       app:layout_constraintStart_toEndOf="@id/last_results_1_distance"
                       app:layout_constraintEnd_toStartOf="@id/last_results_1_time"
                       app:layout_constraintTop_toTopOf="parent"
                       app:layout_constraintBottom_toBottomOf="parent"
                       android:layout_marginStart="5dp"
                       android:gravity="start"
                       style="@style/Body1Roboto14Pt"
                       tools:text="Dowolny"/>
                   <TextView
                       android:id="@+id/last_results_1_time"
                       android:layout_width="0dp"
                       android:layout_height="wrap_content"
                       app:layout_constraintTop_toTopOf="parent"
                       app:layout_constraintBottom_toBottomOf="parent"
                       app:layout_constraintStart_toEndOf="@id/last_results_1_style"
                       app:layout_constraintEnd_toStartOf="@id/last_results_1_points"
                       android:layout_marginEnd="5dp"
                       android:gravity="end"
                       style="@style/Body1Roboto14Pt"
                       tools:text="26.51"/>
                   <TextView
                       android:id="@+id/last_results_1_points"
                       android:layout_width="wrap_content"
                       android:layout_height="wrap_content"
                       app:layout_constraintTop_toTopOf="parent"
                       app:layout_constraintBottom_toBottomOf="parent"
                       app:layout_constraintEnd_toEndOf="parent"
                       style="@style/Body1Roboto14Pt"
                       tools:text="700"/>

               </androidx.constraintlayout.widget.ConstraintLayout>
           </LinearLayout>
            <LinearLayout
                android:id="@+id/last_results_2_container"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">
                <TextView
                    android:id="@+id/last_results_2_header"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    style="@style/CaptionRoboto12Pt"
                    tools:text="I Og\u00f3lnopolskie mistrzostwa O\u0142awy w p\u0142ywaniu"/>
                <androidx.constraintlayout.widget.ConstraintLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:paddingVertical="5dp">
                    <TextView
                        android:id="@+id/last_results_2_distance"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        app:layout_constraintTop_toTopOf="parent"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        style="@style/Body1Roboto14Pt"
                        tools:text="50m" />
                    <TextView
                        android:id="@+id/last_results_2_style"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        app:layout_constraintTop_toTopOf="parent"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintEnd_toStartOf="@id/last_results_2_time"
                        app:layout_constraintStart_toEndOf="@id/last_results_2_distance"
                        android:layout_marginStart="5dp"
                        style="@style/Body1Roboto14Pt"
                        tools:text="Dowolny"
                        />
                    <TextView
                        android:id="@+id/last_results_2_time"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        app:layout_constraintTop_toTopOf="parent"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintEnd_toStartOf="@id/last_results_2_points"
                        app:layout_constraintStart_toEndOf="@id/last_results_2_style"
                        style="@style/Body1Roboto14Pt"
                        android:gravity="end"
                        android:layout_marginEnd="5dp"
                        tools:text="24.56"
                        />
                    <TextView
                        android:id="@+id/last_results_2_points"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        app:layout_constraintTop_toTopOf="parent"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintStart_toEndOf="@id/last_results_2_time"
                        app:layout_constraintEnd_toEndOf="parent"
                        style="@style/Body1Roboto14Pt"
                        tools:text="707"
                        />

                </androidx.constraintlayout.widget.ConstraintLayout>
            </LinearLayout>
        </LinearLayout>
    </androidx.constraintlayout.widget.ConstraintLayout>
</androidx.cardview.widget.CardView>